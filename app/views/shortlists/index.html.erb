<h1>SHORTLIST INDEX BELONGING TO SIGNED IN USER</h1>
<% @shortlists.each do |shortlist| %>
<div class="container">
  <div class="card">
  <div class="card-body">
    <h3 class="card-title">Shortlist Name: <%= shortlist.name %></h3>
    <h4>Artists</h4>
    <% shortlist.artists.each do |artist| %>
    <div class="container">
        <p class="card-text"> - <%= link_to "#{artist.first_name}", artist_path(artist.id) %></p>
    </div>
    <% end %>
    </div>
  </div>
</div>
<% end %>
